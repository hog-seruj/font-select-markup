{
  "name": "postcss-webpack-starter",
  "version": "1.0.0",
  "description": "A webpack boilerplate with configured PostCSS",
  "keywords": [
    "postcss",
    "css",
    "sass",
    "webpack"
  ],
  "main": "src/pcss/main.pcss",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js  --progress --profile --color --display-error-details --display-cached --bail",
    "watch": "cross-env NODE_ENV=development webpack --config ./webpack.config.js --inline --progress --watch --open"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/txantslusam/PostCSS-webpack-starter.git"
  },
  "author": "Mateusz Tomkowiak <matixnt1@windowslive.com>",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.4.2",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^4.0.3",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^4.1.0",
    "postcss-preset-env": "^5.3.0",
    "postcss-property-lookup": "^2.0.0",
    "postcss-url": "^8.0.0",
    "resolve-url-loader": "^3.1.2",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.2",
    "babel-loader": "^8.0.4",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^4.0.0",
    "postcss": "^8.1.0",
    "postcss-custom-media": "^8.0.0",
    "postcss-discard-empty": "^5.0.1",
    "postcss-extend": "^1.0.5",
    "postcss-media-variables": "^2.0.1",
    "twig": "^1.15.4",
    "twig-html-loader": "^0.1.9",
    "twig-loader": "^0.5.5"
  }
}
